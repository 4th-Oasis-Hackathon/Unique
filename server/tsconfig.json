{
  "compilerOptions": {
      "target": "es6",
      "module": "commonjs",
      "pretty": true,
      "sourceMap": true,
      "inlineSources": true,
      "outDir": "dist",
      "importHelpers": true,
      "strict": true,
      "noImplicitAny": false,
      "strictNullChecks": false,
      "noImplicitThis": true,
      "alwaysStrict": true,
      "noUnusedLocals": true,
      "noUnusedParameters": false,
      "noImplicitReturns": true,
      "noFallthroughCasesInSwitch": true,
      "moduleResolution": "node",
      "allowSyntheticDefaultImports": true,
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true,
      "resolveJsonModule": true,
      "esModuleInterop": true,
      "lib": [
          "es5",
          "es6",
          "dom",
          "es2015.core",
          "es2015.collection",
          "es2015.generator",
          "es2015.iterable",
          "es2015.promise",
          "es2015.proxy",
          "es2015.reflect",
          "es2015.symbol",
          "es2015.symbol.wellknown",
          "ES2020.Promise",
          "ES2021.String",
          "esnext.asynciterable"
      ],
  }
}
